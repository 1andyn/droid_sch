    <!-- Create Schedules Activity descriptions -->
    <string name="help_create_search_desc">Search for courses by either CRN or Course Name.</string>
    <string name="help_create_min_course_desc">Sets the minimum class count for schedules.  For example: if you have
        four classes to create schedules with and set the minimum class count to three, you will receive a list of
        possible schedules, some of which contain three classes, and, perhaps, some which contain four classes.</string>
    <string name="help_create_options_desc">Takes you to the \'Preferences\' screen to specify detailed options for
        creating schedules.</string>
    <string name="help_create_profiles_desc">Allows you to select a saved profile to use when creating schedules.  All
        preferences for that profile will be used when creating schedules.</string>
    <string name="help_create_delete_profile_desc">Deletes the selected profile and all of its options.  Cannot be undone.</string>
    <string name="help_create_build_desc">Creates all possible combinations of schedules from your selected courses.</string>
    <string name="help_create_remove_desc">Removes the selected course(s) from the list.</string>
    <string name="help_create_done_desc">Returns to the main menu.</string>

<!-- Strings for View Schedules Activity -->
    <string name="help_view_refresh">Refresh Seating Data:</string>
    <string name="help_view_view">View:</string>
    <string name="help_view_delete">Delete:</string>

    <!-- View Schedules Activity descriptions -->
    <string name="help_view_refresh_desc">Refreshes the available seating data for courses in the saved schedules.  This option is
        accessed from the overflow menu</string>
    <string name="help_view_view_desc">Shows a time chart of the associated schedule as well as a detailed description of each
        course in the schedule.</string>
    <string name="help_view_delete_desc">Deletes the checked schedule(s) from the list.  Cannot be undone.</string>

